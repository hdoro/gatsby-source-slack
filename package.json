{
  "name": "gatsby-source-slack",
  "version": "0.5.3",
  "description": "Source plugin to fetch channels from Slack into Gatsby",
  "main": "gatsby-node.js",
  "scripts": {
    "precommit": "tsc",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx}"
  },
  "keywords": [
    "gatsby",
    "gatsby-source",
    "slack"
  ],
  "author": "Henrique Cavalieri <@hcavalieri>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "emojione": "^4.0.0"
  },
  "devDependencies": {
    "@types/emojione": "^2.2.3",
    "@types/node": "^10.12.2",
    "husky": "^1.1.3",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.8.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.6"
  },
  "peerDependencies": {
    "gatsby": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
